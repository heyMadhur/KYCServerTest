{
    "version":2,
    "builds": [
        {
            "src":"./index.py",
            "use": "@vercel/python",
            "config":{"runtime":"python3.8"}
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "functions": {
    "api/test.js": {
      "maxDuration": 40 
    },
    "api/*.js": {
      "maxDuration": 45 
    },
    "src/api/*.js": {
      "maxDuration": 45 
    }
  }
}